# Database Configuration
# Supported drivers: mysql, postgres, sqlite, sqlite3, sqlserver, mssql
#
# Note: Artisan supports alternate environment variable names:
# - DB_DATABASE or DB_NAME (primary: DB_DATABASE)
# - DB_USERNAME or DB_USER (primary: DB_USERNAME)
# - DB_PASSWORD or DB_PASS (primary: DB_PASSWORD)

# MySQL Configuration
DB_DRIVER=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=your_database
DB_USERNAME=root
DB_PASSWORD=

# PostgreSQL Configuration (uncomment to use)
# DB_DRIVER=postgres
# DB_HOST=localhost
# DB_PORT=5432
# DB_DATABASE=your_database
# DB_USERNAME=postgres
# DB_PASSWORD=

# SQL Server Configuration (uncomment to use)
# DB_DRIVER=sqlserver
# DB_HOST=localhost
# DB_PORT=1433
# DB_DATABASE=your_database
# DB_USERNAME=sa
# DB_PASSWORD=

# SQLite Configuration (uncomment to use)
# DB_DRIVER=sqlite3
# DB_DATABASE=./database.db

# Migration & Seeder Paths
MIGRATIONS_PATH=./database/migrations
SEEDERS_PATH=./database/seeders
